            "type": "package",
            "package": {
                "name": "FortAwesome/Font-Awesome",
                "version": "master",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/FortAwesome/Font-Awesome/archive/master.zip",
                    "type": "zip"
                },
                "require": {
                    "composer/installers": "~1.0"
                }
            }
        }
